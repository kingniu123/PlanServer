<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance http://www.springmodules.org/schema/cache/springmodules-cache.xsd http://www.springmodules.org/schema/cache/springmodules-ehcache.xsd" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>plan</display-name>
  <servlet>
    <servlet-name>login</servlet-name>
    <display-name>login</display-name>
    <description>处理app发送的登录信息</description>
    <servlet-class>plan.login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>register</servlet-name>
    <display-name>注册</display-name>
    <description>处理注册事件</description>
    <servlet-class>plan.register</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>update_users</servlet-name>
    <display-name>更新用户信息</display-name>
    <description>修改用户昵称、电话、邮箱</description>
    <servlet-class>plan.update_users</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>test</servlet-name>
    <display-name>测试数据库</display-name>
    <description>测试数据库</description>
    <servlet-class>plan.test</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getPlans</servlet-name>
    <display-name>获取服务器中全部计划</display-name>
    <description>获取服务器中全部计划</description>
    <servlet-class>plan.getPlans</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getSubPlans</servlet-name>
    <display-name>获取服务器中全部子计划</display-name>
    <description>获取服务器中全部子计划</description>
    <servlet-class>plan.getSubPlans</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>insertPlan</servlet-name>
    <display-name>新建计划</display-name>
    <description>新建计划</description>
    <servlet-class>plan.insertPlan</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>insertSubPlan</servlet-name>
    <display-name>新建子计划</display-name>
    <description>新建子计划</description>
    <servlet-class>plan.insertSubPlan</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>changePsd</servlet-name>
    <display-name>修改密码</display-name>
    <description>修改密码</description>
    <servlet-class>plan.changePsd</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>synchronize_plan</servlet-name>
    <display-name>同步计划到服务器</display-name>
    <description>同步计划到服务器</description>
    <servlet-class>plan.synchronize_plan</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>synchronize_subplan</servlet-name>
    <display-name>同步子计划到服务器</display-name>
    <description>同步子计划到服务器</description>
    <servlet-class>plan.synchronize_subplan</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getUsers</servlet-name>
    <display-name>获取用户信息</display-name>
    <description>获取用户信息</description>
    <servlet-class>plan.getUsers</servlet-class>
  </servlet>












  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/servlet/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/servlet/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>update_users</servlet-name>
    <url-pattern>/servlet/update_users</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/servlet/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getPlans</servlet-name>
    <url-pattern>/servlet/getPlans</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getSubPlans</servlet-name>
    <url-pattern>/servlet/getSubPlans</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>insertPlan</servlet-name>
    <url-pattern>/servlet/insertPlan</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>insertSubPlan</servlet-name>
    <url-pattern>/servlet/insertSubPlan</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>changePsd</servlet-name>
    <url-pattern>/servlet/changePsd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>synchronize_plan</servlet-name>
    <url-pattern>/servlet/synchronize_plan</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>synchronize_subplan</servlet-name>
    <url-pattern>/servlet/synchronize_subplan</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getUsers</servlet-name>
    <url-pattern>/servlet/getUsers</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>